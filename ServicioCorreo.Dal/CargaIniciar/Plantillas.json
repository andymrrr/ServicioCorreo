  {
    "Nombre": "Plantilla Registro Usuario",
    "ContenidoHtml": "<!DOCTYPE html>\n<html lang=\"es\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Registro de Usuario</title>\n  <style>\n    body {\n      font-family: Arial, sans-serif;\n      margin: 0;\n      padding: 0;\n      background-color: #f4f4f4;\n    }\n    .container {\n      max-width: 700px;\n      margin: 20px auto;\n      background: #ffffff;\n      border-radius: 8px;\n      box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n      padding: 20px;\n    }\n    .header {\n      text-align: center;\n      padding: 10px 0;\n      background-color: #333333;\n      color: #ffffff;\n      border-radius: 8px 8px 0 0;\n    }\n    .header h1 {\n      margin: 0;\n      font-size: 24px;\n    }\n    .details {\n      margin: 20px 0;\n      padding: 20px;\n      background-color: #f9f9f9;\n      border-radius: 8px;\n    }\n    .details h2 {\n      font-size: 20px;\n      color: #333;\n    }\n    .details p {\n      margin: 5px 0;\n      font-size: 16px;\n    }\n    .footer {\n      text-align: center;\n      margin-top: 20px;\n      font-size: 14px;\n      color: #888;\n    }\n  </style>\n</head>\n<body>\n  <div class=\"container\">\n    <!-- Encabezado -->\n    <div class=\"header\">\n      <h1>Bienvenido, {{NombreUsuario}}</h1>\n    </div>\n    \n    <!-- Detalles del usuario -->\n    <div class=\"details\">\n      <h2>Detalles del Registro</h2>\n      <p><strong>Nombre Completo:</strong> {{NombreCompleto}}</p>\n      <p><strong>Correo Electrónico:</strong> {{CorreoElectronico}}</p>\n      <p><strong>Fecha de Registro:</strong> {{FechaRegistro}}</p>\n      <p><strong>Estado:</strong> {{Estado}}</p>\n    </div>\n    \n    <!-- Instrucciones -->\n    <div class=\"footer\">\n      <p>Gracias por registrarte en nuestra plataforma.</p>\n      <p>Si tienes preguntas, contáctanos a {{CorreoSoporte}}.</p>\n    </div>\n  </div>\n</body>\n</html>",
    "ParametrosEsperados": "[\"NombreUsuario\", \"NombreCompleto\", \"CorreoElectronico\", \"FechaRegistro\", \"Estado\", \"CorreoSoporte\"]",
    "FechaCreacion": "2024-12-02T00:00:00",
    "FechaUltimaModificacion": null,
    "Descripcion": "Plantilla para notificar el registro de un usuario.",
    "Activo": true
  },
  {
    "Nombre": "Plantilla Factura",
    "ContenidoHtml": "<!DOCTYPE html>\n<html lang=\"es\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Factura</title>\n  <style>\n    body {\n      font-family: Arial, sans-serif;\n      margin: 0;\n      padding: 0;\n      background-color: #f4f4f4;\n    }\n    .container {\n      max-width: 700px;\n      margin: 20px auto;\n      background: #ffffff;\n      border-radius: 8px;\n      box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n      padding: 20px;\n    }\n    .header {\n      text-align: center;\n      padding: 10px 0;\n      background-color: #333333;\n      color: #ffffff;\n      border-radius: 8px 8px 0 0;\n    }\n    .header img {\n      max-width: 100px;\n      margin-bottom: 10px;\n    }\n    .header h1 {\n      margin: 0;\n      font-size: 24px;\n    }\n    .details {\n      margin: 20px 0;\n      padding: 20px;\n      background-color: #f9f9f9;\n      border-radius: 8px;\n    }\n    .details h2 {\n      font-size: 20px;\n      color: #333;\n    }\n    .details p {\n      margin: 5px 0;\n      font-size: 16px;\n    }\n    table {\n      width: 100%;\n      border-collapse: collapse;\n      margin: 20px 0;\n    }\n    table th, table td {\n      padding: 10px;\n      border: 1px solid #ddd;\n      text-align: left;\n    }\n    table th {\n      background-color: #333;\n      color: #fff;\n    }\n    table tfoot td {\n      font-weight: bold;\n    }\n    .footer {\n      text-align: center;\n      margin-top: 20px;\n      font-size: 14px;\n      color: #888;\n    }\n  </style>\n</head>\n<body>\n  <div class=\"container\">\n    <!-- Encabezado -->\n    <div class=\"header\">\n      <img src=\"{{LogoUrl}}\" alt=\"Logo de la empresa\">\n      <h1>Factura</h1>\n    </div>\n    \n    <!-- Detalles del cliente y empresa -->\n    <div class=\"details\">\n      <h2>Detalles</h2>\n      <p><strong>Cliente:</strong> {{NombreCliente}}</p>\n      <p><strong>Empresa:</strong> {{NombreEmpresa}}</p>\n      <p><strong>Fecha:</strong> {{Fecha}}</p>\n      <p><strong>Número de Factura:</strong> {{NumeroFactura}}</p>\n      <p><strong>Estado:</strong> {{Estado}}</p>\n    </div>\n    \n    <!-- Tabla de productos/servicios -->\n    <table>\n      <thead>\n        <tr>\n          <th>Descripción</th>\n          <th>Cantidad</th>\n          <th>Precio Unitario</th>\n          <th>Subtotal</th>\n        </tr>\n      </thead>\n      <tbody>\n        {{#Items}}\n        <tr>\n          <td>{{Descripcion}}</td>\n          <td>{{Cantidad}}</td>\n          <td>{{PrecioUnitario}}</td>\n          <td>{{Subtotal}}</td>\n        </tr>\n        {{/Items}}\n      </tbody>\n      <tfoot>\n        <tr>\n          <td colspan=\"3\">Total</td>\n          <td>{{Total}}</td>\n        </tr>\n      </tfoot>\n    </table>\n    \n    <!-- Pie de página -->\n    <div class=\"footer\">\n      <p>Gracias por su preferencia.</p>\n      <p>Si tiene preguntas sobre esta factura, contáctenos a {{CorreoEmpresa}}.</p>\n    </div>\n  </div>\n</body>\n</html>",
    "ParametrosEsperados": "[\"LogoUrl\", \"NombreCliente\", \"NombreEmpresa\", \"Fecha\", \"NumeroFactura\", \"Items\", \"Total\", \"CorreoEmpresa\", \"Estado\"]",
    "FechaCreacion": "2024-12-02T00:00:00",
    "FechaUltimaModificacion": null,
    "Descripcion": "Plantilla para la generación de facturas.",
    "Activo": true
  },
